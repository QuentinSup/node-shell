				"required": true
			},
			{
				"cmd": "svn up",
				"cwd": "../datalogger",
				"required": false
			}],
			"TS42": [
			{
				"cmd": "svn up",
				"cwd": ".",
				"required": false
			}, {
				"cmd": "svn up",
				"cwd": "../datalogger",
				"required": false
			}, {
